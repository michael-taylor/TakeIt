{
  "name": "writeit-src",
  "version": "0.1.0",
  "description": "WriteIt theme source file",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run build:tailwind --watch\"",
    "build": "hugo --verbose --source=exampleSite --themesDir ../.. --gc",
    "build:preview": "hugo --logLevel=info --source=exampleSite --themesDir ../.. -D --gc",
    "server": "hugo server --source=exampleSite --themesDir ../.. -D --disableFastRender",
    "server:production": "hugo server --source=exampleSite --themesDir ../.. --disableFastRender -e production",
    "server:tailwind": "npx tailwindcss --watch -i ./assets/css/tailwind.css -o ./assets/css/main.css",
    "algolia": "atomic-algolia",
    "build:tailwind": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css",
    "publish": "hugo --source=exampleSite --themesDir ../.. --destination docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michael-taylor/WriteIt.git"
  },
  "keywords": [
    "hugo",
    "theme"
  ],
  "author": "Michael R. Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michael-taylor/WriteIt/issues"
  },
  "homepage": "https://github.com/michael-taylor/WriteIt",
  "dependencies": {
    "atomic-algolia": "^0.3.19",
    "autocomplete.js": "^0.38.1",
    "clipboard": "^2.0.11",
    "cookieconsent": "^3.1.1",
    "echarts": "^5.4.3",
    "katex": "^0.16.8",
    "node-fetch": "^3.3.2",
    "tablesort": "^5.3.0",
    "twemoji": "^14.0.2",
    "twikoo": "^1.6.17",
    "valine": "^1.5.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "eslint": "^8.47.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "tailwindcss": "^3.3.3"
  }
}
